{
	"useTabs": false,
	"singleQuote": false,
	"trailingComma": "all",
	"semi": true,
	"tabWidth": 2,
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"tailwindAttributes": [],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"tailwindStylesheet": "./src/app.css"
}
